  <Sequence sap2010:Annotation.AnnotationText="This sample shows how to invoke VBA from within a workflow.&#xA;It returns the names of the first 3 sheets in a workbook.&#xD;&#xA;&#xD;&#xA;Note: Trust Access to the VBA project object model must be enabled from Excel; otherwise an error is thrown. See https://activities.uipath.com/docs/invoke-vba for instruction." DisplayName="Invoke VBA sample" sap2010:WorkflowViewState.IdRef="Sequence_2">
    <ui:ExcelApplicationScope Password="{x:Null}" DisplayName="Excel Application Scope" sap2010:WorkflowViewState.IdRef="ExcelApplicationScope_1" WorkbookPath="Test.xlsx">
      <ui:ExcelApplicationScope.Body>
        <ActivityAction x:TypeArguments="ui:WorkbookApplication">
          <ActivityAction.Argument>
            <DelegateInArgument x:TypeArguments="ui:WorkbookApplication" Name="ExcelWorkbookScope" />
          </ActivityAction.Argument>
          <Sequence DisplayName="Do" sap2010:WorkflowViewState.IdRef="Sequence_1">
            <Sequence.Variables>
              <Variable x:TypeArguments="x:Object" Name="sheets" />
              <Variable x:TypeArguments="s:String[]" Name="sheetNames" />
            </Sequence.Variables>
            <ui:InvokeVBA sap2010:Annotation.AnnotationText="CodeFilePath: the VBA code being invoked can be put in a .txt file and indicated in this input property.&#xA;&#xA;EntryMethodName: this is the function/sub name that is going to be called.&#xA;&#xA;EntryMethodParameters: this is the parameters that are going to be passsed to the function/sub.&#xA;&#xA;OutputValue: any output will be saved in this property." CodeFilePath="VBA.txt" DisplayName="Invoke VBA" EntryMethodName="GetFirstNSheetNames" EntryMethodParameters="[{3}]" sap2010:WorkflowViewState.IdRef="InvokeVBA_1" OutputValue="[sheets]" />
            <Assign sap2010:Annotation.AnnotationText="We know the function we called returns a string array." DisplayName="Convert to string array" sap2010:WorkflowViewState.IdRef="Assign_1">
              <Assign.To>
                <OutArgument x:TypeArguments="s:String[]">[sheetNames]</OutArgument>
              </Assign.To>
              <Assign.Value>
                <InArgument x:TypeArguments="s:String[]">[CType(sheets, String())]</InArgument>
              </Assign.Value>
            </Assign>
            <WriteLine DisplayName="Write sheet names" sap2010:WorkflowViewState.IdRef="WriteLine_1" Text="[String.Join(&quot;,&quot;, sheetNames)]" />
          </Sequence>
        </ActivityAction>
      </ui:ExcelApplicationScope.Body>
    </ui:ExcelApplicationScope>
